@model IEnumerable<Aprendizado.Class.perguntasProva>

@{
    ViewBag.Title = "prepararSorteio";
}

<h2>prepararSorteio</h2>


<body>
  <a  name="services"></a>
    <div class="content-section-a">

        <div class="container">
            <div class="row">
               
                    <hr class="section-heading-spacer">

                    <div class="clearfix"></div>

        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Teste perguntas da atividade automatica
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th>
                                        Identificacao
                                    </th>

                                    <th>
                                        Titulo
                                    </th>
                                    <th>
                                        Enunciado
                                    </th>
                                    <th></th>
                                </tr>

@foreach (var item in Model)
{
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.Identificacao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Titulo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Enunciado)
        </td>
        
    </tr>
}
 </table>

                    </div>
                    <!-- /.table-responsive -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        </div>  
            </div>
        </div>
</body>



