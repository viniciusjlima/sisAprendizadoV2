@model IEnumerable<Aprendizado.Class.ErradaTema>


@{
    ViewBag.Title = "errosPorTema";
}


<div id="page-wrapper">
        <div id="page-inner">
            <div class="row">
                <div class="col-md-12">
                    <h1 class="page-header">Relatórios de Desempenho<small> - Erros por Tema - @ViewBag.NomePessoa</small>
                    </h1>
                </div>
            </div>
            <!-- /. ROW  -->

            <div class="row">
                <div class="col-md-12">
                    <!-- Advanced Tables -->
                    <div class="panel panel-default">
                        <div class="panel-body">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                    <thead>
                                         <tr>


        <th>
            Tema
        </th>
        <th>
            Tema
        </th>
        
    </tr>
            
        </thead>
        <tbody>
   @foreach (var item in Model)
{
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.Tema)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.QtdErradas)
        </td>

    </tr>
}
                                    </tbody>
                                </table>
                            </div>
                            <input type="button"  class="btn btn-default" value="Voltar" onClick="    history.go(-1)"> 
                        </div>
                    </div>
                    <!--End Advanced Tables -->
                </div>
            </div>
        </div>
    </div>
